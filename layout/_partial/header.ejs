<header>
  <div class="outer">
    <div class="inner">
      <h1 class="logo-wrap">
        <a><%= theme.header.title %> ProWiki<b><sup><%= theme.header.version %></sup></b></a>
      </h1>
    </div>
    <div class="inner">
      <nav class="main-nav">
        <% for (var i in theme.menus){ %>
          <a href="<%- url_for(theme.menus[i]) %>"><%= i %></a>
        <% } %>
      </nav>
    </div>
  </div>
</header>